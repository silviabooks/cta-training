{
  "resource_version": 13,
  "metadata": {
    "version": 3,
    "sources": [
      {
        "name": "default",
        "kind": "postgres",
        "tables": [
          {
            "table": {
              "schema": "public",
              "name": "expenses"
            },
            "object_relationships": [
              {
                "name": "user",
                "using": {
                  "foreign_key_constraint_on": "user_id"
                }
              }
            ],
            "select_permissions": [
              {
                "role": "anonymous",
                "permission": {
                  "columns": [
                    "amount",
                    "created_at",
                    "id",
                    "notes"
                  ],
                  "filter": {
                    "user": {
                      "is_public": {
                        "_eq": true
                      }
                    }
                  }
                }
              },
              {
                "role": "user",
                "permission": {
                  "columns": [
                    "amount",
                    "id",
                    "notes"
                  ],
                  "filter": {
                    "user_id": {
                      "_eq": "X-Hasura-User-Id"
                    }
                  },
                  "allow_aggregations": true
                }
              }
            ]
          },
          {
            "table": {
              "schema": "public",
              "name": "users"
            },
            "array_relationships": [
              {
                "name": "expenses",
                "using": {
                  "foreign_key_constraint_on": {
                    "column": "user_id",
                    "table": {
                      "schema": "public",
                      "name": "expenses"
                    }
                  }
                }
              }
            ],
            "insert_permissions": [
              {
                "role": "anonymous",
                "permission": {
                  "check": {},
                  "set": {
                    "is_public": "true"
                  },
                  "columns": [
                    "name"
                  ],
                  "backend_only": false
                }
              }
            ],
            "select_permissions": [
              {
                "role": "anonymous",
                "permission": {
                  "columns": [
                    "created_at",
                    "name"
                  ],
                  "filter": {
                    "is_public": {
                      "_eq": true
                    }
                  },
                  "limit": 50
                }
              },
              {
                "role": "user",
                "permission": {
                  "columns": [
                    "id",
                    "name"
                  ],
                  "filter": {
                    "id": {
                      "_eq": "X-Hasura-User-Id"
                    }
                  },
                  "limit": 1
                }
              }
            ]
          }
        ],
        "configuration": {
          "connection_info": {
            "use_prepared_statements": true,
            "database_url": {
              "from_env": "HASURA_GRAPHQL_DATABASE_URL"
            },
            "isolation_level": "read-committed",
            "pool_settings": {
              "connection_lifetime": 600,
              "retries": 1,
              "idle_timeout": 180,
              "max_connections": 50
            }
          }
        }
      }
    ],
    "actions": [
      {
        "name": "multi",
        "definition": {
          "handler": "{{MULTI_BASE_URL}}/multi",
          "output_type": "MultiOutput",
          "arguments": [
            {
              "name": "num1",
              "type": "Int!"
            },
            {
              "name": "num2",
              "type": "Int!"
            }
          ],
          "type": "query"
        },
        "comment": "Multiply numbers"
      },
      {
        "name": "names",
        "definition": {
          "handler": "{{MULTI_BASE_URL}}/names",
          "output_type": "[NamesOutput]!",
          "type": "query"
        },
        "permissions": [
          {
            "role": "anonymous"
          }
        ]
      }
    ],
    "custom_types": {
      "objects": [
        {
          "name": "MultiOutput",
          "fields": [
            {
              "name": "result",
              "type": "Int!"
            }
          ]
        },
        {
          "name": "NamesOutput",
          "fields": [
            {
              "name": "value",
              "type": "String!"
            }
          ]
        }
      ]
    }
  }
}